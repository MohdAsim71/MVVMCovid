<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mexcelle.data">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.mexcelle.presentation.App"
        android:icon="@mipmap/logofinal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logofinal"
        android:supportsRtl="true"
        android:allowBackup="false"
        android:theme="@style/Theme.MVVMCovid">
        <activity android:name="com.mexcelle.countryActivity.CountryActivity"></activity>
        <activity android:name="com.mexcelle.data.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>